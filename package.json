{
    "name": "tsharplanguage-color",
    "displayName": "t# highlighter",
    "publisher": "akamurasaki",
    "description": "Function for highlighting T#.",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.58.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "tsharp",
                "aliases": [
                    "t#",
                    "t#"
                ],
                "extensions": [
                    ".t#"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "tsharp",
                "scopeName": "source.tsharp",
                "path": "./syntaxes/tsharp.tmLanguage.json"
            }
        ]
    },
    "dependencies": {}
}
