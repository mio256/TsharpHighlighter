{
    "name": "tsharplanguage-color",
    "displayName": "t# highlighter",
    "description": "Function for highlighting T#.",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.62.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "tsharp",
            "aliases": ["t#", "t#"],
            "extensions": [".t#"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "tsharp",
            "scopeName": "source.tsharp",
            "path": "./syntaxes/tsharp.tmLanguage.json"
        }]
    }
}