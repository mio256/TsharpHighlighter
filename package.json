{
    "name": "tsharplanguage-color",
    "displayName": "t# highlighter",
    "publisher": "akamurasaki",
    "repository": {
        "type": "git",
        "url": "https://github.com/Tsharp-lang/TsharpHighlighter"
    },
    "icon": "img/tsharp_logo.PNG",
    "description": "highlighter for T#.",
    "version": "0.0.5",
    "engines": {
        "vscode": "^1.58.0"
    },
    "license": "No License",
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "tsharp",
                "aliases": [
                    "t#",
                    "t#"
                ],
                "extensions": [
                    ".t#"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "tsharp",
                "scopeName": "source.tsharp",
                "path": "./syntaxes/tsharp.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "tsharp",
                "scopeName": "source.tsharp",
                "path": "./snippets/tsharp.json"
            }
        ]
    },
    "dependencies": {}
}
